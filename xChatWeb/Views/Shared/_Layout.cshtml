    <script type="text/x-handlebars-template" id="AgentsHandlebars">
        {{# each lstAgents}}

        <a href="#" class="listAgent chat-list-item d-flex flex-row w-100 p-2 border-bottom" id="btnconnect" style="color:black" data-AgentValueId="{{AgentId}}" data-IsAgent="{{AccountManagerIsAgent}}" data-agentname="{{AccountAgentName}}" ondrop="drop(event)" ondragover="allowDrop(event)">
            <div class="chat-list-item d-flex flex-row w-100 p-2" data-id="{{AgentId}}" id="lstAgentData">
                <img src="../Content/Imagenes/imgdefault.png" alt="Profile Photo" class="img-fluid rounded-circle mr-2" style="height:50px;">
