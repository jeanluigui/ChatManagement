                    <div class="chat-list-item d-flex flex-row w-100 p-2" data-userid="{{userId}}" data-username="{{userName}}" data-chatid="{{ChatId}}" data-statusid="{{Status}}" id="lstUserData" draggable="true" ondragstart="drag(event)">
